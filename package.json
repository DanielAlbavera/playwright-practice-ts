{
  "name": "playwright-practice-ts",
  "version": "1.0.0",
  "description": "Automated Framework with Playwright and TypeScript",
  "main": "index.js",
  "scripts": {
    "test" : "npx playwright test",
    "test:line" : "npx playwright test --reporter=line",
    "test:trace" : "npx playwright test --trace on",
    "test:visual" : "npx playwright test visual-comparison.spec",
    "update:spanshots" : "npx playwright test visual-comparison.spec --update-snapshots"
  },
  "keywords": ["Automation testing", "Playwright", "e2e"],
  "author": "Daniel Albavera",
  "license": "ISC",
  "devDependencies": {
    "@playwright/test": "^1.31.0"
  },
  "dependencies": {
    "dotenv": "^16.0.3"
  }
}
